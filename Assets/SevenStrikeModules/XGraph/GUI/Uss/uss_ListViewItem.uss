.item_label_title
{
     position: relative;
     color: rgb(255, 255, 255);
     align-self: auto;
     flex-grow: 1;
     -unity-text-align: middle-right;
     margin-right: 10px;
     margin-top: 0;
     padding-right: 0;
     height: 10px;
     top: auto;
     margin-left: 10px;
     margin-bottom: 0;
     padding-top: 0;
     padding-bottom: 0;
     padding-left: 0;
}

.item_icon
{
     width: 5px;
     height: 5px;
     background-color: rgb(59, 254, 155);
     position: relative;
     align-self: auto;
     flex-shrink: 0;
     flex-grow: 0;
     border-top-left-radius: 6px;
     border-top-right-radius: 6px;
     border-bottom-right-radius: 6px;
     border-bottom-left-radius: 6px;
     top: auto;
     left: auto;
     justify-content: center;
     margin-left: 10px;
     border-left-color: rgb(0, 0, 0);
     border-right-color: rgb(0, 0, 0);
     border-top-color: rgb(0, 0, 0);
     border-bottom-color: rgb(0, 0, 0);
     flex-direction: column;
     align-items: flex-start;
     margin-top: 0;
     margin-right: 0;
     margin-bottom: 0;
}

.item_label_des
{
     -unity-text-align: middle-right;
     color: rgba(255, 255, 255,0.4);
     font-size: 12px;
     margin-top: 0;
     padding-top: 0;
     padding-bottom: 0;
     margin-bottom: 0;
     text-overflow: clip;
     overflow: hidden;
     width: auto;
     padding-left: 0;
     margin-left: 30px;
     align-items: stretch;
     flex-grow: 1;
     margin-right: 0;
     padding-right: 0;
}

.item_container_pill
{
     border-top-left-radius: 13px;
     border-top-right-radius: 13px;
     border-bottom-right-radius: 13px;
     border-bottom-left-radius: 13px;
     border-top-width: 1px;
     border-right-width: 1px;
     border-bottom-width: 1px;
     border-left-width: 1px;
     border-left-color: rgb(20, 20, 20);
     border-right-color: rgb(20, 20, 20);
     border-top-color: rgb(20, 20, 20);
     border-bottom-color: rgb(20, 20, 20);
     flex-direction: row;
     flex-shrink: 0;
     align-items: center;
     height: 30px;
     background-color: rgb(39, 39, 39);
}

.item_container_des
{
     flex-direction: row;
     flex-grow: 1;
     width: 100%;
}

.item_container
{
     background-size: 100% 100%;
     background-color: rgba(0, 0, 0, 0);
     width: auto;
     height: auto;
     justify-content: flex-start;
     margin-bottom: 0;
     margin-right: 0;
     padding-right: 5px;
     flex-grow: 0;
     flex-shrink: 0;
     flex-direction: row;
}

.item_container:hover .item_container_pill
{
     border-top-color: rgba(59, 254, 155,0.4);
     border-bottom-color: rgba(59, 254, 155,0.4);
     border-left-color: rgba(59, 254, 155,0.4);
     border-right-color: rgba(59, 254, 155,0.4);
}

.item_container:checked .item_container_pill
{
     border-top-color: rgba(59, 254, 155,1);
     border-bottom-color: rgba(59, 254, 155,1);
     border-left-color: rgba(59, 254, 155,1);
     border-right-color: rgba(59, 254, 155,1);
}
